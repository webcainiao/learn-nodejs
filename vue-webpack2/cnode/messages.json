      },
      "create_at": "2017-05-12T11:05:16.734Z"
    }, {
      "id": "59158d93ba8670562a40f05a",
      "type": "reply",
      "has_read": true,
      "author": {
        "loginname": "jiangli373",
        "avatar_url": "https://avatars1.githubusercontent.com/u/1632862?v=3&s=120"
      },
      "topic": {
        "id": "591587e93504ce1c2ac45bbf",
